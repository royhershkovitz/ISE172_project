        <Label x:Name="L_Omri" Content="Omri Meshulami" HorizontalAlignment="Left" Margin="201,242,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="36"/>
        <Button x:Name="B_Back" Content="Back" HorizontalAlignment="Left" Margin="288,319,0,0" VerticalAlignment="Top" Width="108" Height="33" FontWeight="Bold" FontSize="22" Click="B_Back_Click"/>

    </Grid>
